<odoo>
    <data>
        <record id="product_mass_upload_status" model="ir.ui.view">
            <field name="name">product.status.form.wizard</field>
            <field name="model">mass.upload.status</field>
            <field name="arch" type="xml">
                <form string="Mass Uplaod Product Status">
                    <group>
                        <h4><field name="message" readonly="1"/></h4>
                    </group>
                    <footer>
                        <button special="cancel" class="pull-right" string="Close"/>
                        <button name="view_history" string="View History" class="btn-primary pull-right" type="object" />
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>